# Google Analytics 4 Measurement ID
# Get this from your Google Analytics property
NEXT_PUBLIC_GOOGLE_ANALYTICS=G-XXXXXXXXXX

# Google Search Console Verification
# Replace with your actual verification code from Google Search Console
NEXT_PUBLIC_GOOGLE_SITE_VERIFICATION=your-google-verification-code-here

# Site URL for production
NEXT_PUBLIC_SITE_URL=https://your-domain.vercel.app

# Other optional tracking IDs
NEXT_PUBLIC_HOTJAR_ID=YOUR_HOTJAR_ID
NEXT_PUBLIC_CLARITY_ID=YOUR_CLARITY_ID
